<?xml version="1.0" encoding="utf-8" ?>
<!-- Custom source for units of measurement -->
<!-- This one includes base and some derived SI units and also prefixed units -->
<unitCollection>
  <standardPrefixes>
    <prefix multiplier="1000000000000000000000000"  namePrefix="yotta" symbolPrefix="Y" />
    <prefix multiplier="1000000000000000000000"     namePrefix="zetta" symbolPrefix="Z" />
    <prefix multiplier="1000000000000000000"        namePrefix="exa"   symbolPrefix="E" />
    <prefix multiplier="1000000000000000"           namePrefix="peta"  symbolPrefix="P" />
    <prefix multiplier="1000000000000"              namePrefix="tera"  symbolPrefix="T" />
    <prefix multiplier="1000000000"                 namePrefix="giga"  symbolPrefix="G" />
    <prefix multiplier="1000000"                    namePrefix="mega"  symbolPrefix="M" />
    <prefix multiplier="1000"                       namePrefix="kilo"  symbolPrefix="k" />
    <prefix multiplier="100"                        namePrefix="hecto" symbolPrefix="h" />
    <prefix multiplier="10"                         namePrefix="deca"  symbolPrefix="da" />
    <prefix multiplier="0.1"                        namePrefix="deci"  symbolPrefix="d" />
    <prefix multiplier="0.01"                       namePrefix="centi" symbolPrefix="c" />
    <prefix multiplier="0.001"                      namePrefix="milli" symbolPrefix="m" />
    <prefix multiplier="0.000001"                   namePrefix="micro" symbolPrefix="µ" />
    <prefix multiplier="0.000000001"                namePrefix="nano"  symbolPrefix="n" />
    <prefix multiplier="0.000000000001"             namePrefix="pico"  symbolPrefix="p" />
    <prefix multiplier="0.000000000000001"          namePrefix="femto" symbolPrefix="f" />
    <prefix multiplier="0.000000000000000001"       namePrefix="atto"  symbolPrefix="a" />
    <prefix multiplier="0.000000000000000000001"    namePrefix="zepto" symbolPrefix="z" />
    <prefix multiplier="0.000000000000000000000001" namePrefix="yocto" symbolPrefix="y" />
  </standardPrefixes>
  <units>
    <unit name="metre"          symbol="m"   addStandardPrefixes="True" />
    <unit name="kilogram"       symbol="kg"  addStandardPrefixes="False">
      <prefixed multiplier="1000000000000000000000"        name="yottagram (exatonne)"  symbol="Yg" />
      <prefixed multiplier="1000000000000000000"           name="zettagram (petatonne)" symbol="Zg" />
      <prefixed multiplier="1000000000000000"              name="exagram (teratonne)"   symbol="Eg" />
      <prefixed multiplier="1000000000000"                 name="petagram (gigatonne)"  symbol="Pg" />
      <prefixed multiplier="1000000000"                    name="teragram (megatonne)"  symbol="Tg" />
      <prefixed multiplier="1000000"                       name="gigagram (kilotonne)"  symbol="Gg" />
      <prefixed multiplier="1000"                          name="megagram (tonne)"      symbol="Mg" />
      <prefixed multiplier="100"                           name="centner (quintal)"     symbol="qq" />
      <prefixed multiplier="0.1"                           name="hectogram"             symbol="hg" />
      <prefixed multiplier="0.01"                          name="decagram"              symbol="dag" />
      <prefixed multiplier="0.001"                         name="gram"                  symbol="g" />
      <prefixed multiplier="0.0001"                        name="decigram"              symbol="dg" />
      <prefixed multiplier="0.00001"                       name="centigram"             symbol="cg" />
      <prefixed multiplier="0.000001"                      name="milligram"             symbol="mg" />
      <prefixed multiplier="0.000000001"                   name="microgram"             symbol="µg" />
      <prefixed multiplier="0.000000000001"                name="nanogram"              symbol="ng" />
      <prefixed multiplier="0.000000000000001"             name="picogram"              symbol="pg" />
      <prefixed multiplier="0.000000000000000001"          name="femtogram"             symbol="fg" />
      <prefixed multiplier="0.000000000000000000001"       name="attogram"              symbol="ag" />
      <prefixed multiplier="0.000000000000000000000001"    name="zeptogram"             symbol="zg" />
      <prefixed multiplier="0.000000000000000000000000001" name="yoctogram"             symbol="yg" />
    </unit>
    <unit name="second"         symbol="s"   addStandardPrefixes="True">
      <prefixed multiplier="31557600"                   name="Julian year" symbol="a" />
      <prefixed multiplier="86400"                      name="day"         symbol="d" />
      <prefixed multiplier="3600"                       name="hour"        symbol="h" />
      <prefixed multiplier="60"                         name="minute"      symbol="min" />
    </unit>
    <unit name="ampere"         symbol="A"   addStandardPrefixes="True" />
    <unit name="kelvin"         symbol="K"   addStandardPrefixes="True" />
    <unit name="mole"           symbol="mol" addStandardPrefixes="True" />
    <unit name="candela"        symbol="cd"  addStandardPrefixes="True" />
    <unit name="hertz"          symbol="Hz"  addStandardPrefixes="True" />
    <unit name="radian"         symbol="rad" addStandardPrefixes="False" />
    <unit name="steradian"      symbol="sr"  addStandardPrefixes="False" />
    <unit name="newton"         symbol="N"   addStandardPrefixes="True" />
    <unit name="pascal"         symbol="Pa"  addStandardPrefixes="True" />
    <unit name="joule"          symbol="J"   addStandardPrefixes="True" />
    <unit name="watt"           symbol="W"   addStandardPrefixes="True" />
    <unit name="coulomb"        symbol="C"   addStandardPrefixes="True" />
    <unit name="volt"           symbol="V"   addStandardPrefixes="True" />
    <unit name="farad"          symbol="F"   addStandardPrefixes="True" />
    <unit name="ohm"            symbol="Ω"   addStandardPrefixes="True" />
    <unit name="siemens"        symbol="S"   addStandardPrefixes="True" />
    <unit name="weber"          symbol="Wb"  addStandardPrefixes="True" />
    <unit name="tesla"          symbol="T"   addStandardPrefixes="True" />
    <unit name="degree Celsius" symbol="°C"  addStandardPrefixes="False" />
    <unit name="lux"            symbol="lx"  addStandardPrefixes="True" />
    <unit name="becquerel"      symbol="Bq"  addStandardPrefixes="True" />
    <unit name="gray"           symbol="Gy"  addStandardPrefixes="True" />
    <unit name="sievert"        symbol="Sv"  addStandardPrefixes="True" />
    <unit name="katal"          symbol="kat" addStandardPrefixes="True" />
  </units>
</unitCollection>